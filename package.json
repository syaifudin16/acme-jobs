  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "studio": "bunx prisma studio",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
